{
  "projectType": "library",
  "sourceRoot": "libs/weather-lib/src",
  "prefix": "ngrx-weather-update",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/weather-lib"],
      "options": {
        "jestConfig": "libs/weather-lib/jest.config.ts",
        "passWithNoTests": true
      }
