    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/weather-lib/**/*.ts",
          "libs/weather-lib/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
